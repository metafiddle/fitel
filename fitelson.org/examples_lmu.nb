Cell["Example #1", "Subsection",
 CellChangeTimes->{{3.410479429514853*^9, 3.410479445274488*^9}}],

Cell["\<\
The first example of a probability model that we saw was the following:\
\>", "Text",
 CellChangeTimes->{{3.410479470171661*^9, 3.41047948333704*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"MODEL1", " ", "=", " ", 
  RowBox[{"PrSAT", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"Pr", "[", 
       RowBox[{"X", "\[And]", "Y"}], "]"}], "==", 
      FractionBox["1", "6"]}], ",", 
     RowBox[{
      RowBox[{"Pr", "[", 
       RowBox[{"X", "\[And]", 
        RowBox[{"\[Not]", "Y"}]}], "]"}], "==", 
      FractionBox["1", "4"]}], ",", 
     RowBox[{
      RowBox[{"Pr", "[", 
       RowBox[{
        RowBox[{"\[Not]", "X"}], "\[And]", "Y"}], "]"}], "==", 
      FractionBox["1", "8"]}], ",", 
     RowBox[{
      RowBox[{"Pr", "[", 
       RowBox[{
        RowBox[{"\[Not]", "X"}], "\[And]", 
        RowBox[{"\[Not]", "Y"}]}], "]"}], "==", 
      FractionBox["11", "24"]}]}], "}"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.410479484993518*^9, 3.410479535525733*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"X", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        SubscriptBox["\[DoubleStruckA]", "2"], ",", 
        SubscriptBox["\[DoubleStruckA]", "4"]}], "}"}]}], ",", 
     RowBox[{"Y", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        SubscriptBox["\[DoubleStruckA]", "3"], ",", 
        SubscriptBox["\[DoubleStruckA]", "4"]}], "}"}]}], ",", 
     RowBox[{"\[CapitalOmega]", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        SubscriptBox["\[DoubleStruckA]", "1"], ",", 
        SubscriptBox["\[DoubleStruckA]", "2"], ",", 
        SubscriptBox["\[DoubleStruckA]", "3"], ",", 
        SubscriptBox["\[DoubleStruckA]", "4"]}], "}"}]}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[DoubleStruckA]", "1"], "\[Rule]", 
      FractionBox["11", "24"]}], ",", 
     RowBox[{
      SubscriptBox["\[DoubleStruckA]", "2"], "\[Rule]", 
      FractionBox["1", "4"]}], ",", 
     RowBox[{
      SubscriptBox["\[DoubleStruckA]", "3"], "\[Rule]", 
      FractionBox["1", "8"]}], ",", 
     RowBox[{
      SubscriptBox["\[DoubleStruckA]", "4"], "\[Rule]", 
      FractionBox["1", "6"]}]}], "}"}]}], "}"}]], "Output",
 CellChangeTimes->{3.410479536714077*^9, 3.410733800171772*^9, 
  3.410733868324669*^9, 3.488685225620973*^9, 3.517056497958987*^9, 
  3.5170565544108458`*^9, 3.517057014532724*^9, 3.5171628586329823`*^9}]
}, Open  ]],

