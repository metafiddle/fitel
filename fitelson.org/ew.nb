  FormBox[
   FractionBox[
    RowBox[{
     SubsuperscriptBox["P", "i", "1"], "(", 
     RowBox[{
      RowBox[{"q", "&"}], "p"}], ")"}], 
    RowBox[{
     SubsuperscriptBox["P", "i", "1"], "(", "p", ")"}]], TraditionalForm]]],
 "=",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["0.325", 
     RowBox[{"0.325", "+", "0.225"}]], "="}], TraditionalForm]]],
 " 0.590909 \[NotEqual] ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{
      SubsuperscriptBox["P", "1", "0"], "(", 
      RowBox[{"q", "|", "p"}], ")"}], "+", 
     RowBox[{
      SubsuperscriptBox["P", "2", "0"], "(", 
      RowBox[{"q", "|", "p"}], ")"}]}], "2"], TraditionalForm]]],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     FractionBox[
      RowBox[{
       SubsuperscriptBox["P", "1", "0"], "(", 
       RowBox[{
        RowBox[{"q", "&"}], "p"}], ")"}], 
      RowBox[{
       SubsuperscriptBox["P", "1", "0"], "(", "p", ")"}]], "+", 
     FractionBox[
      RowBox[{
       SubsuperscriptBox["P", "2", "0"], "(", 
       RowBox[{
        RowBox[{"q", "&"}], "p"}], ")"}], 
      RowBox[{
       SubsuperscriptBox["P", "2", "0"], "(", "p", ")"}]]}], "2"], 
   TraditionalForm]]],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
