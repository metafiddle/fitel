Cell[BoxData[{
 RowBox[{
  RowBox[{"states", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"\[GothicCapitalC]", "\[And]", "P", "\[And]", "Q"}], ",", 
     RowBox[{"\[GothicCapitalC]", "\[And]", "P", "\[And]", 
      RowBox[{"\[Not]", "Q"}]}], ",", 
     RowBox[{"\[GothicCapitalC]", "\[And]", 
      RowBox[{"\[Not]", "P"}], "\[And]", "Q"}], ",", 
     RowBox[{"\[GothicCapitalC]", "\[And]", 
      RowBox[{"\[Not]", "P"}], "\[And]", 
      RowBox[{"\[Not]", "Q"}]}], ",", 
     RowBox[{
      RowBox[{"\[Not]", "\[GothicCapitalC]"}], "\[And]", "P", "\[And]", "Q"}],
      ",", 
     RowBox[{
      RowBox[{"\[Not]", "\[GothicCapitalC]"}], "\[And]", "P", "\[And]", 
      RowBox[{"\[Not]", "Q"}]}], ",", 
     RowBox[{
      RowBox[{"\[Not]", "\[GothicCapitalC]"}], "\[And]", 
      RowBox[{"\[Not]", "P"}], "\[And]", "Q"}], ",", 
     RowBox[{
      RowBox[{"\[Not]", "\[GothicCapitalC]"}], "\[And]", 
      RowBox[{"\[Not]", "P"}], "\[And]", 
      RowBox[{"\[Not]", "Q"}]}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"props", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"Or", "@@", "#"}], "&"}], "/@", 
    RowBox[{"Subsets", "[", "states", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"test", "[", "p_", "]"}], ":=", 
   RowBox[{
    RowBox[{"Not", "[", 
     RowBox[{
      RowBox[{"BooleanMinimize", "[", 
       RowBox[{"p", "&&", "P"}], "]"}], "===", "False"}], "]"}], "&&", 
    RowBox[{"!", 
     RowBox[{"TautologyQ", "[", 
      RowBox[{"Implies", "[", 
       RowBox[{
        RowBox[{"p", "&&", "P"}], ",", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"\[Not]", "Q"}], "&&", "\[GothicCapitalC]"}], ")"}], "||", 
         
         RowBox[{"(", 
          RowBox[{"Q", "&&", 
           RowBox[{"\[Not]", "\[GothicCapitalC]"}]}], ")"}]}]}], "]"}], 
      "]"}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"PconProps", "=", 
   RowBox[{"Drop", "[", 
    RowBox[{
     RowBox[{"BooleanMinimize", "/@", 
      RowBox[{"Select", "[", 
       RowBox[{"props", ",", 
        RowBox[{
         RowBox[{"test", "[", "#", "]"}], "&"}]}], "]"}]}], ",", 
     RowBox[{"-", "1"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"RESILIENT", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Pr", "[", 
       RowBox[{"\[GothicCapitalC]", "|", 
        RowBox[{
        "PconProps", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]}],
        "]"}], "\[Equal]", 
      RowBox[{"Pr", "[", 
       RowBox[{"Q", "|", 
        RowBox[{"BooleanMinimize", "[", 
         RowBox[{"P", "&&", 
          RowBox[{
          "PconProps", "\[LeftDoubleBracket]", "i", 
           "\[RightDoubleBracket]"}]}], "]"}]}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"Length", "[", "PconProps", "]"}]}], "}"}]}], "]"}]}], 
  ";"}]}], "Input",
 CellChangeTimes->{{3.6209545323081284`*^9, 3.620954708670005*^9}, {
