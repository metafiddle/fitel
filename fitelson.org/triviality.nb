Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"PrSAT", "[", 
  RowBox[{"{", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Lewis", "'"}], "s", " ", "Two", " ", "Instances", " ", "of", 
     " ", "The", " ", "Resilient", " ", "Equation"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"Pr", "[", 
      RowBox[{"\[GothicCapitalC]", "|", 
       RowBox[{"\[Not]", "Q"}]}], "]"}], "\[Equal]", "0"}], ",", " ", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{"let", " ", "X"}], " ", "=", " ", 
      RowBox[{"\[Not]", "Q"}]}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Pr", "[", 
      RowBox[{"\[GothicCapitalC]", "|", " ", "Q"}], "]"}], " ", "\[Equal]", 
     "1"}], ",", 
    RowBox[{"(*", " ", 
     RowBox[{
