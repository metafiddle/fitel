      FractionBox["1", "6"]}], ",", 
     RowBox[{
      RowBox[{"Pr", "[", 
       RowBox[{"X", "\[And]", 
        RowBox[{"\[Not]", "Y"}]}], "]"}], "==", 
      FractionBox["1", "4"]}], ",", 
     RowBox[{
      RowBox[{"Pr", "[", 
       RowBox[{
        RowBox[{"\[Not]", "X"}], "\[And]", "Y"}], "]"}], "==", 
      FractionBox["1", "8"]}], ",", 
     RowBox[{
